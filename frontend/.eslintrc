{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": 1,
    "no-empty": 1,
    "react/prop-types": 1,
    "react/display-name": 0,
    "no-debugger": 1,
    "no-unused-vars": 1,
    "no-undef": 1,
    "no-useless-escape": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
